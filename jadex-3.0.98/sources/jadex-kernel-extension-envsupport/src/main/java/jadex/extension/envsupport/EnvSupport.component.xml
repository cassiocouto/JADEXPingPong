	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.activecomponents.org/jadex-component
						http://www.activecomponents.org/jadex-component-${jadex_build_version}.xsd"
	name="EnvSupport" package="jadex.extension.envsupport">

	<imports>
		<import>jadex.bridge.service.types.factory.*</import>
	</imports>
			
	<services>
		<providedservice name="envextension" class="IComponentFactoryExtensionService">
			<implementation class="EnvSupportExtensionService"/>
		</providedservice>
	</services>
	
</componenttype>